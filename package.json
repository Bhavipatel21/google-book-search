{
  "name": "google-book-search",
  "version": "1.0.0",
  "description": "MERN app for google book search",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run prod || npm run dev",
    "prod": "node server.js",
    "dev": "react-scripts start",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "dotenv": "^10.0.0",
    "express": "^4.16.3",
    "if-env": "^1.0.4",
    "mongoose": "^5.3.16",
    "serve": "^12.0.0"
  }
}
